<Window x:Class="SYSTools_Update_Tools_.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:muxc="http://schemas.inkore.net/lib/ui/wpf/modern"
        xmlns:local="clr-namespace:SYSTools_Update_Tools_"
        mc:Ignorable="d"
        muxc:ThemeManager.IsThemeAware="True"
        muxc:TitleBar.ExtendViewIntoTitleBar="False"    
        muxc:WindowHelper.SystemBackdropType="Mica"
        muxc:TitleBar.IsBackButtonVisible="False"
        muxc:WindowHelper.UseModernWindowStyle="True"
        Title="ToolsUpdate 工具包更新器" Loaded="Window_Loaded" Height="200" Width="410" ShowInTaskbar="False" ResizeMode="NoResize">
    <Grid>
        <TextBlock x:Name="Version" TextWrapping="Wrap" HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="18" Margin="0,35,0,0" Height="25" />
        <TextBlock x:Name="Cloud_Version" TextWrapping="Wrap" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="18" Text="等待获取版本更新中..."/>
        <TextBlock x:Name="Version_Verify" TextWrapping="Wrap" HorizontalAlignment="Center" VerticalAlignment="Bottom" FontSize="18" Margin="0,0,0,35"/>
    </Grid>
</Window>
